{
  "name": "battleship",
  "version": "1.0.0",
  "description": "Battleship Game",
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ],
    "src/**/*.{html,css}": [
      "prettier --write --ignore-unknown"
    ]
  },
  "scripts": {
    "start": "webpack serve --config webpack.dev.js",
    "build": "webpack serve --config webpack.prod.js",
    "dev": "webpack --config webpack.dev.js",
    "prod": "webpack --config webpack.dev.js",
    "prettier": "prettier --write ./src/",
    "prettier-global": "prettier --write .",
    "lint": "eslint ./src/",
    "lint-fix": "eslint --fix ./src/",
    "prepare": "husky",
    "test": "jest"
  },
  "repository": {
    "type": "git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.29.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "mini-css-extract-plugin": "^2.9.2",
    "prettier": "^3.6.2",
    "style-loader": "^4.0.0",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "interact": "^0.0.3"
  }
}
